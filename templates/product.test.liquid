{% endcomment %}

{% assign lable_out = true %}
{% if product.available  %}
{% assign lable_out = false %}
{% endif %}
{% assign number_of_related_products = section.settings.number_of_related_products %}
{% assign related_products_title = section.settings.related_products_title %}

<div class="product-bg">
  {% assign content = product.description | remove: "[disable-icon-new]" %}
  <div class="container">
    <div class="row pro-layout1" id="product">
      {% if section.settings.enable_block_right %}
      <div class="col-xs-12 col-main col-md-9 col-sm-9">
        {% else %}
        <div class="col-main col-xs-12 col-md-12 col-sm-12 col-lg-12">
          {% endif %}
          <div itemscope itemtype="http://schema.org/Product" class="product">
            <meta itemprop="url" content="{{ shop.url }}{{ product.url }}">
            <meta itemprop="image" content="{{ product.featured_image.src | product_img_url: 'master' }}">
            <meta itemprop="shop-currency" content="{{ shop.currency }}">

            <div class="row product-img-box">
       
          
              <div class="col-xs-12 col-md-6 col-sm-12 product-img-box product-img-blur hidden-xs">
               	<div class="images pr oh images--{{ image_class }} images--{{ image_class }}-{{ section.settings.thumbnail_position }} images--grid">
                    {% if product.images.size > 0 %}
                      <div class="product-single__image{% if product.images.size > 1 %} slick{% endif %}" data-slick='{"slidesToShow": 1, "slidesToScroll": 1, "asNavFor": ".product-single__thumbnail", "fade":true}'>
                          {% for image in product.images %}
                           
                              <a href="#" data-image="{{ image.src | img_url: 'master' }}" data-size="{{ image.width }}x{{ image.height }}" class="product-single__photos-item{% if zoom %} jas-zoom{% endif %} jas-photoswipe" data-photoswipe-index="{{ forloop.index0 }}">
                          
                                  <img class="w__100" src="{{ image.src | img_url: '570x' }}" alt="{{ image.alt | escape }}">
                              </a>
                          {% endfor %}
                      </div>
                  {% else %}
                      {{ 'image' | placeholder_svg_tag: 'placeholder-svg' }}
                  {% endif %}

                     {% if product.images.size > 1 %}	
                        <div class="product-single__thumbnail oh slick" {{ slick_thumb }}>
                          {% for image in product.images %}
                              {% for variant in image.variants %}
                                  <div class="product-single__thumbnail-item product-single__thumbnail-variant" data-variant-id="{{ variant.id }}">
                                      <img
                                          src="{{ image.src | img_url: '160x215' }}"
                                          alt="{{ image.alt | escape }}"
                                      />
                                  </div>
                              {% else %}
                                  <div class="product-single__thumbnail-item">
                                      <img
                                          src="{{ image.src | img_url: '160x215' }}"
                                          alt="{{ image.alt | escape }}"
                                      />
                                  </div>
                              {% endfor %}
                          {% endfor %}
                      </div>
                    {% endif %}
                </div>
              </div>
             
